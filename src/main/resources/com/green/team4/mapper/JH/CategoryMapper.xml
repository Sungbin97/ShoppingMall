<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.team4.mapper.JH.CategoryMapper">
    <select id="cateList" resultType="com.green.team4.vo.JH.CategoryVO">

        select * from tbl_product_cate order by pCateCode

    </select>

    <select id="getCateCode1" resultType="com.green.team4.vo.JH.CategoryVO">

		<![CDATA[
        select * from tbl_product_cate where pCateCode > 100000 and pCateCode < 200000
        ]]>
	</select>

    <select id="getCateCode2" resultType="com.green.team4.vo.JH.CategoryVO">

		<![CDATA[
        select * from tbl_product_cate where pCateCode > 200000 and pCateCode < 300000
        ]]>

	</select>

    <select id="getCateTier2" resultType="com.green.team4.vo.JH.CategoryVO">
        select * from tbl_product_cate where tier='2';
	</select>

</mapper>